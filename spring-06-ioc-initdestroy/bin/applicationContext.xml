<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
         	<!--
         	destroy-method 销毁该对象的方法
            -->
	<bean class="com.shine.spring.initdestroy.HelloWorld" id="helloworld" init-method="init" destroy-method="destroy"></bean>
	
	<bean class="com.shine.spring.initdestroy.HelloWorld2" id="helloworld2" init-method="init" destroy-method="destroy" scope="prototype"></bean>
</beans>